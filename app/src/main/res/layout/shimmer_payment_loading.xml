<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/recycling1"
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="50dp"
                app:cardBackgroundColor="@color/blackgray"
                android:layout_height="50dp"
                app:cardCornerRadius="50dp">
            </androidx.cardview.widget.CardView>
            <FrameLayout
                android:background="@color/blackgray"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="75dp">

            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/recycling2"
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@id/recycling1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="50dp"
                app:cardBackgroundColor="@color/blackgray"
                android:layout_height="50dp"
                app:cardCornerRadius="50dp">
            </androidx.cardview.widget.CardView>
            <FrameLayout
                android:background="@color/blackgray"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="75dp">

            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/recycling3"
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@id/recycling2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="50dp"
                app:cardBackgroundColor="@color/blackgray"
                android:layout_height="50dp"
                app:cardCornerRadius="50dp">
            </androidx.cardview.widget.CardView>
            <FrameLayout
                android:background="@color/blackgray"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="75dp">

            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/recycling4"
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@id/recycling3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="50dp"
                app:cardBackgroundColor="@color/blackgray"
                android:layout_height="50dp"
                app:cardCornerRadius="50dp">
            </androidx.cardview.widget.CardView>
            <FrameLayout
                android:background="@color/blackgray"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="75dp">

            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>